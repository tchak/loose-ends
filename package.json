{
  "private": true,
  "name": "loose-ends",
  "description": "",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "dev:start": "node -r dotenv/config node_modules/.bin/remix dev",
    "dev:css": "tailwindcss -o ./app/styles/tailwind.css --watch",
    "cf": "cross-env NODE_ENV=development run-p cf:dev:*",
    "cf:dev:remix": "remix watch",
    "cf:dev:wrangler": "wrangler pages dev ./public --watch ./netlify/functions/server/build",
    "build": "cross-env NODE_ENV=production run-s build:*",
    "build:clean": "del .cache netlify/functions/server/build public/build app/styles",
    "build:css": "tailwindcss -o ./app/styles/tailwind.css",
    "build:remix": "remix build",
    "build:size": "size-limit",
    "lint": "run-s lint:*",
    "lint:types": "tsc",
    "lint:js": "eslint . --ext .ts,.tsx",
    "start": "remix-serve ./netlify/functions/server/build",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.5",
    "@netlify/functions": "^0.10.0",
    "@reach/menu-button": "^0.16.2",
    "@remix-run/cloudflare-pages": "^1.1.1",
    "@remix-run/netlify": "^1.1.1",
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "@supabase/supabase-js": "^1.28.5",
    "@vvo/tzdb": "^6.39.0",
    "is-hotkey": "^0.2.0",
    "luxon": "^2.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1",
    "remix-auth": "^2.5.1",
    "remix-utils": "^2.2.0",
    "use-debounce": "^7.0.1",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@axe-core/react": "^4.3.2",
    "@cloudflare/workers-types": "^3.2.0",
    "@remix-run/dev": "^1.1.1",
    "@size-limit/preset-app": "^7.0.4",
    "@types/is-hotkey": "^0.1.6",
    "@types/luxon": "^2.0.8",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "cross-env": "^7.0.3",
    "del-cli": "^4.0.1",
    "dotenv": "^10.0.0",
    "esbuild": "0.13.14",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-typestrict": "^1.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "size-limit": "^7.0.4",
    "tailwindcss": "^3.0.7",
    "typescript": "^4.1.2",
    "wrangler": "alpha"
  },
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "main": "dist/worker.js",
  "size-limit": [
    {
      "path": "public/build/**/*.js"
    }
  ]
}
